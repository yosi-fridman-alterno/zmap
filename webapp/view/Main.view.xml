<mvc:View controllerName="zmap.zmap.controller.Main" displayBlock="true" xmlns:vbm="sap.ui.vbm" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<vbm:GeoMap id="GeoMap" width="100%" height="100%">
							<vbm:vos>
								<vbm:Routes>
									<vbm:Route position="34.796025;32.0719388;0; 34.8570187;32.0220433;0" color="rgb(117,148,34)" colorBorder="rgb(255,255,255)" linewidth="3"
										routetype="Geodesic" click="onClickRoute" contextMenu="onContextMenuRoute"/>
									<vbm:Route position="34.8570187;32.0220433;0; 34.6665169;31.8181307;0" color="rgb(92,186,230)" colorBorder="rgb(255,255,255)" linewidth="3"
										routetype="Geodesic" lineDash="10;5" click="onClickRoute" contextMenu="onContextMenuRoute"/>
								</vbm:Routes>
							</vbm:vos>
						</vbm:GeoMap>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>